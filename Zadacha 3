#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    setlocale(LC_ALL, "Russian");
    double x1,x2, a, b, c,d;
    cin >> a >> b >> c;
    if ((d = b * b - 4 * a * c) > 0 and a!=0 and b!=0 and c!=0) {
        x1 = (- b + sqrt(d)) / 2 / a;
        x2 = (-b - sqrt(d)) / 2 / a;
        cout << x1 << " " << x2;
    }
    if ((d = b * b - 4 * a * c) == 0 and a!=0 and b!=0 and c!=0) {
        x1 = -b / 2 / a;
        cout << x1;
    }if ((d = b * b - 4 * a * c) < 0 and a != 0 and b != 0 and c != 0) {
        cout << "Нет решений";
    }if (a == 0 and b != 0 and c != 0) {
        x1 = (-c) / b;
        cout << x1;
    }if (b == 0 and a > 0 and c > 0) {
        cout << "Нет решений";
    }if (b == 0 and a < 0 and c < 0) {
        cout << "Нет решений";
    }if (b == 0 and a < 0 and c > 0) {
        x1 = sqrt(-c / a);
        cout << x1;
    }
    if (b == 0 and a > 0 and c < 0) {
        x1 = sqrt(-c / a);
        cout << x1;
    }if (c == 0 and a != 0 and b != 0) {
        x1 = 0;
        x2 = -b / a;
        cout << x1 << " " << x2;
    }if (a == 0 and b == 0 and c == 0) {
        cout << "x - любое число";
    }
}
